datasource db {
    provider = "mongodb"
    url = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

model news {
    id String @id @default(auto()) @map("_id") @db.ObjectId
    title String
    description String
    date DateTime @default(now())
    content String
    images String[]
    thumbnail String
}